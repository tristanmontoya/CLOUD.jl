   "source": [
    "using PyPlot\n",
    "pygui(true)\n",
    "# use x = linspace(0,2*pi,1000) in Julia 0.6\n",
    "x = range(0; stop=2*pi, length=1000); y = sin.(3 * x + 4 * cos.(2 * x));\n",
    "plot(x, y, color=\"red\", linewidth=2.0, linestyle=\"--\")\n",
    "title(\"A sinusoidally modulated sinusoid\")\n",
    "show()"
   ]
  }
